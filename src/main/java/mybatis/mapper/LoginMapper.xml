<?xml version="1.0" encoding="UTF-8"?>


<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
 <mapper namespace="impl.AcademyInfoImpl">
 
	<!-- 회원로그인 -->
	<select id="memberLogin" resultType="mybatis01.MemberDTO"
		parameterType="mybatis01.MemberDTO">
		SELECT * FROM NormalMember
		<if test="!memberId.equals('') and memberId!=null">
			<if test="!memberPass.equals('') and memberPass!=null">
				where MemberId=#{memberId} AND MemberPass=#{memberPass}
			</if>
		</if>
	</select>
	
</mapper>